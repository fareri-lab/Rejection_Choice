Qualtrics.SurveyEngine.addOnload(function()
{
	/*Place your JavaScript here to run when the page loads*/

});

Qualtrics.SurveyEngine.addOnReady(function()
{
	//photo 2
	var that = this;
		var photo = "${q://QID15/UploadedFileName}"; //photo 1
		var msg = "ALERT: DUPLICATE DETECTED";
    jQuery("#NextButton").on('click',function(){
        var uploadedfile = jQuery("#"+that.questionId+" input[type='FileUpload']:Uploaded").attr("FileName");
        if(uploadedfile==photo){
        alert(msg);
        }

    });

});

Qualtrics.SurveyEngine.addOnUnload(function()
{
	/*Place your JavaScript here to run when the page is unloaded*/

});

<input type='file' id='UploadFile'>
if (document.getElementById("UploadFile").files  === photo) {
  console.log("no files selected");
}

